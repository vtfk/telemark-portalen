  },
  "bugs": {
    "url": "https://github.com/vtfk/telemark-portalen/issues"
  },
  "dependencies": {
    "@babel/polyfill": "7.7.0",
    "@date-io/date-fns": "1.3.13",
    "@hot-loader/react-dom": "16.11.0",
    "@loadable/component": "5.11.0",
    "@material-ui/core": "4.8.2",
    "@material-ui/pickers": "3.2.8",
    "core-js": "3.6.1",
    "date-fns": "2.8.1",
    "dayjs": "1.8.18",
    "isomorphic-unfetch": "3.0.0",
    "object.omit": "3.0.0",
    "prop-types": "15.7.2",
    "querystringify": "2.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet-async": "1.0.4",
    "react-hot-loader": "4.12.18",
    "react-markdown": "4.3.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@loadable/babel-plugin": "5.11.0",
    "customize-cra": "0.9.1",
    "eslint-plugin-prettier": "3.1.2",
    "http-proxy-middleware": "0.20.0",
    "husky": "3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.3.0",
    "webpack-bundle-analyzer": "3.6.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "now-dev": "BROWSER=none react-scripts start",
    "dev": "now dev",
    "deploy": "now --prod",
    "build": "NODE_ENV=production react-scripts build",
    "analyze": "ANALYZE=1 NODE_ENV=production react-app-rewired build",
    "lint": "eslint --no-eslintrc -c .eslintrc src api",
    "test": "react-scripts test",
    "format": "prettier --write \"src/**/*.js\" \"api/**/*.js\" \"config-overrides.js\"",
    "precommit": "pretty-quick --staged",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "postinstall": "cd api && npm install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.18.0"
  }
}
